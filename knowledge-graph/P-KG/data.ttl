    ex:dateGraduated "2020-04-01"^^xsd:date .

ex:Award_DeansList a ex:Award ;
    schema:name "Dean's List" ;
    schema:description "Recognized for outstanding academic performance." .

ex:INFO202 a ex:Course ;
    schema:name "Information Organization" .

ex:Enroll_INFO202 a ex:CourseEnrollment ;
    schema:about ex:INFO202 ;
    ex:dateTaken "2024-01-15"^^xsd:date ;
