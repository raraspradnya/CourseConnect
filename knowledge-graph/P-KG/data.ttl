    ex:dateGraduated "2021-06-10"^^xsd:date .

ex:Python a ex:Skill ;
    schema:name "Python" ;
    ex:experienceLevel "Intermediate" .

ex:ProjectManagement a ex:Skill ;
    schema:name "Project Management" ;
    ex:experienceLevel "Advanced" .

ex:SQL a ex:Skill ;
    schema:name "SQL" ;
    ex:experienceLevel "Advanced" .

ex:DataViz a ex:Skill ;
    schema:name "Data Visualization" ;
    ex:experienceLevel "Intermediate" .

ex:AI a ex:Interest ; schema:name "Artificial Intelligence" .
ex:UX a ex:Interest ; schema:name "User Experience" .
